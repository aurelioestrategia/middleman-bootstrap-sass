<meta name="description" content="<%= current_page.data.description %>">
<meta name="keywords" content="<%= current_page.data.keywords %>">
<meta name="viewport" content="width=device-width, initial-scale=1">
        
<meta name="application-name" content="<%= current_page.data.apname %>"/>

<!-- Open Graph data --> 
<meta property="og:title" content="<%= current_page.data.ogtitle %>"/>
<meta property="og:description" content="<%= current_page.data.ogdescription %>"/>
<meta property="og:url" content="<%= current_page.data.ogurl %>"/>
<meta property="og:type" content="<%= current_page.data.ogtype %>"/>
<meta property="og:image" content="<%= current_page.data.ogimage %>"/>
<meta property="og:site_name" content="<%= current_page.data.ogsitename %>"/>

<!-- Twitter Card data -->
<meta name="twitter:card" content="<%= current_page.data.twcard %>"/>
<meta name="twitter:site" content="<%= current_page.data.twsite %>"/>
<meta name="twitter:title" content="<%= current_page.data.twtitle %>"/>
<meta name="twitter:image" content="<%= current_page.data.twimage %>"/>
<meta name="twitter:description" content="<%= current_page.data.twdescription %>"/>

<!-- Schema.org markup for Google+ -->
<meta itemprop="name" content="<%= current_page.data.scname %>">
<meta itemprop="description" content="<%= current_page.data.scdescription %>"/>
<meta itemprop="image" content="<%= current_page.data.scimage %>"/>